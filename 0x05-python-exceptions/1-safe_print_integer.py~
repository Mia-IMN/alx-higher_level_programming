#!/usr/bin/python3

# Function that prints an integer with "{:d}".format()

def safe_print_integer(value):
    isInt = True
    try:
        print("{:d}".format(value))
        #return True
    except ValueError:
        isInt = False
        #return False
    if isInt:
        #print("{}".format(value))
        return True
    else:
        return False
